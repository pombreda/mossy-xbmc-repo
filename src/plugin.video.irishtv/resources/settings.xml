<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<settings>		
	<!-- General -->
	<category label="30100">
	 <setting id="select_action" type="labelenum" label="30110" default="Play" lvalues="30120|30130|30140"/>
	 <setting id="bitrate" type="labelenum" label="40390" default="Default" lvalues="40400|40410|40420|40430|40440|40450|40460|40470|40480|40490"/>
