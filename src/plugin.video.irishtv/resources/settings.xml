	  <!-- Proxy -->
    <category label="30300">
         <setting label="30370" type="lsep"/>
         <setting id="forward_segment1" label="30362" type="slider" range="1,1,255" default="46" option="int"/>
         <setting id="forward_segment2" label="30363" type="slider" range="0,1,255" default="7" option="int"/>
         <setting label="30307" type="lsep"/>
         <setting id="proxy_type" label="30315" type="enum" values="HTTP|HTTP (Tunneled)|SOCKS4|SOCKS5" default="0" />
         <setting id="proxy_server" label="30320" type="text" default=""/>
         <setting id="proxy_port" label="30330" type="number" default=""/>
         <setting id="proxy_user" label="30340" type="text"/>
         <setting id="proxy_pass" label="30350" type="text" option="hidden" />
	</category>
    <category label="30305">
         <setting id="RTE_proxy_method" label="20100" type="enum" values="None|Forwarded IP|Proxy for HTTP|Proxy for HTTP and Streams" default="0" />
         <setting id="TV3_proxy_method" label="20101" type="enum" values="None|Forwarded IP|Proxy for HTTP|Proxy for HTTP and Streams" default="0" />
         <setting id="AerTV_proxy_method" label="20102" type="enum" values="None|Forwarded IP|Proxy for HTTP|Proxy for HTTP and Streams" default="0" />
         <setting id="TG4_proxy_method" label="20103" type="enum" values="None|Forwarded IP|Proxy for HTTP|Proxy for HTTP and Streams" default="0" />
	</category>

	<!-- Downloads -->
	<category label="30200">
	 <setting id="rtmpdump_path" type="file" label="30210" default=""/>
